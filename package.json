{
  "name": "vue-localer",
  "version": "1.3.0",
  "description": "Internationalization plugin for Vue.",
  "main": "./dist/vue-localer.umd.cjs",
  "module": "./dist/vue-localer.js",
  "types": "./dist/vue-localer.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/vue-localer.js",
